fieldset#default-step-0.step(title="Items", style="display: block;")
    legend Which Items  are your disputing?
    .col-sm-12
        .panel.panel-default
                .panel-heading
                    div(style="float:right; margin-top:-4px; margin-right:50px;")




                        Label(for="all_furnishers") All Furnishers
                        input(type="checkbox")#all_furnishers
                        Label(for="all_equifax") All Equifax
                        input(type="checkbox")#all_equifax
                        Label(for="all_experian") All Experian
                        input(type="checkbox")#all_experian
                        Label(for="all_Transunion") All Transunion
                        input(type="checkbox")#all_transunion

                    h3.panel-title Original Creditors
                        .actions.pull-right
                            i.fa.fa-chevron-up.clientData
                .panel-body(style="padding:0;padding-top:15px;padding-bottom:15px")
                    table.table
                        tr
                            th Item AccountNumber
                            th Equifax
                            th Experian
                            th Transunion
                            th Source
                        each accounts
                            tr
                                td {{itemData.account.equifax.accountNumber}}
                                td {{itemData.account.equifax.accountNumber}}

                                td
                                    input(type="checkbox")
                                td
                                    input(type="checkbox")
                                td
                                    input(type="checkbox")
                                td
                                    input(type="checkbox")



